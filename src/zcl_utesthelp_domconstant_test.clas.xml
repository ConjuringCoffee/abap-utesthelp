    <LANGU>E</LANGU>
    <DESCRIPT>Reusable unit test for constant classes of domains</DESCRIPT>
    <CATEGORY>05</CATEGORY>
    <STATE>1</STATE>
    <CLSCCINCL>X</CLSCCINCL>
    <FIXPT>X</FIXPT>
    <UNICODE>X</UNICODE>
    <WITH_UNIT_TESTS>X</WITH_UNIT_TESTS>
    <DURATION_TYPE>12</DURATION_TYPE>
    <RISK_LEVEL>11</RISK_LEVEL>
   </VSEOCLASS>
   <DESCRIPTIONS>
    <SEOCOMPOTX>
     <CMPNAME>ALL_CONSTANTS_FOUND_IN_DOMAIN</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Each constant must have a fix value in the comain</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>ALL_CONSTANTS_HAVE_SAME_TYPE</CMPNAME>
     <LANGU>E</LANGU>
